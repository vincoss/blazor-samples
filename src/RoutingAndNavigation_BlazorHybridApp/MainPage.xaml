<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:BlazorRoutingAndNavigation_MauiBlazorHybridApp"
             x:Class="BlazorRoutingAndNavigation_MauiBlazorHybridApp.MainPage">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html" StartPath="/weather">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type local:Components.Routes}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>
